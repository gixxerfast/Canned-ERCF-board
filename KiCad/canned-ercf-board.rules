
(rules PCB canned-ercf-board
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 1103)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 3.5)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.4)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_1000:600_um"
    (shape
      (circle F.Cu 1000.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 1000.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_1000:600_um" "Via[0-1]_1000:600_um" default
  )
  (via 
    "Via[0-1]_1000:600_um-kicad_default" "Via[0-1]_1000:600_um" "kicad_default"
  )
  (via 
    "Via[0-1]_1000:600_um-MOT" "Via[0-1]_1000:600_um" MOT
  )
  (via 
    "Via[0-1]_1000:600_um-PWR" "Via[0-1]_1000:600_um" PWR
  )
  (via_rule
    default "Via[0-1]_1000:600_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_1000:600_um-kicad_default"
  )
  (via_rule
    MOT "Via[0-1]_1000:600_um-MOT"
  )
  (via_rule
    PWR "Via[0-1]_1000:600_um-PWR"
  )
  (class default
    ENCODER SERVO EXTRA "unconnected-(A1-Pad2)" "unconnected-(A1-Pad3)" "unconnected-(A1-Pad4)" "unconnected-(U2-PadJP5_1)" "unconnected-(U3-PadJP3_1)"
    "unconnected-(U3-PadJP5_1)" "DIAG_0" "DIAG_1" "DIR_1" "DRIVES_EN" "DIR_0" "STEP_0" "unconnected-(A1-Pad13)"
    "STEP_1" "CAN_H" "CAN_L" "unconnected-(A1-Pad19)" "unconnected-(A1-Pad20)" "unconnected-(A1-Pad21)" "unconnected-(A1-Pad22)" "unconnected-(A1-Pad23)"
    ENDSTOP "DRIVES_UART" "unconnected-(A1-Pad26)" TX "unconnected-(A1-Pad28)" "unconnected-(A1-Pad29)" "unconnected-(A1-Pad30)" "unconnected-(A1-Pad31)"
    "Net-(JP1-Pad2)" "Net-(JP2-Pad3)" "Net-(JP2-Pad1)" "Net-(JP3-Pad3)" "Net-(JP3-Pad1)" "unconnected-(U2-PadJP3_1)" "Net-(D1-Pad1)" "Net-(D2-Pad1)"
    "Net-(D3-Pad1)"
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class MOT
    "Net-(J7-Pad4)" "Net-(J7-Pad3)" "Net-(J7-Pad2)" "Net-(J7-Pad1)" "Net-(J8-Pad1)" "Net-(J8-Pad2)" "Net-(J8-Pad3)" "Net-(J8-Pad4)"
    (clearance_class MOT)
    (via_rule MOT)
    (rule
      (width 1000.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class PWR
    GND VDC +5V +3V3
    (clearance_class PWR)
    (via_rule PWR)
    (rule
      (width 800.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)